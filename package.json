{
  "name": "@peachjar/action-run-migrations",
  "version": "0.0.1",
  "private": true,
  "description": "Deploy to a Peachjar Environment",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "pack": "ncc build",
    "test": "jest --config jest.config.js --forceExit",
    "act": "act -s GITHUB_DEPLOY_USER -s GITHUB_DEPLOY_TOKEN -s AWS_ACCESS_KEY_ID -s AWS_SECRET_ACCESS_KEY",
    "coverage": "./node_modules/.bin/jest --config jest.config.js --forceExit --coverage --verbose",
    "all": "npm run build && npm run lint && npm run pack && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "rclayton-the-terrible",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.3",
    "@actions/github": "^2.1.0",
    "joi": "^14.3.1",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/joi": "^14.3.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.7.12",
    "@zeit/ncc": "^0.20.5",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "jest-junit": "^6.2.1",
    "js-yaml": "^3.13.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "typescript": "^3.6.4"
  },
  "peachjar": {
    "migrations": [
      {
        "image": "noop",
        "tag": "db2",
        "secret": "flyway-noop-postgres-env"
      }
    ]
  }
}
